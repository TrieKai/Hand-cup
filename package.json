{
  "name": "hand-cup-project",
  "version": "0.0.0",
  "scripts": {
    "start:heroku": "node dist/handCup-project-server/main.js",
    "postinstall": "npm run build:ssr",
    "heroku-prebuild": "rm -rf node_modules && npm cache clean --force",
    "heroku-postbuild": "ng build --prod",
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --port 9527",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --color",
    "serve:ssr": "node dist/handCup-project-server/main.js",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run handCup-project:server:production"
  },
  "private": true,
